{
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
    "packages": {
        ".": {
            "skip-github-release": false,
            "release-type": "node",
            "package-name": "demo",
            "include-component-in-tag": false,
            "release-branches": [
                "main"
            ],
            "pull-request-title-pattern": "chore(release): v${version}",
            "changelog-sections": [
                {
                    "type": "feat",
                    "section": "Features",
                    "hidden": false
                },
                {
                    "type": "fix",
                    "section": "Bug Fixes",
                    "hidden": false
                },
                {
                    "type": "chore",
                    "section": "Other",
                    "hidden": false
                },
                {
                    "type": "docs",
                    "section": "Documentation",
                    "hidden": false
                },
                {
                    "type": "style",
                    "section": "Style",
                    "hidden": false
                },
                {
                    "type": "refactor",
                    "section": "Refactoring",
                    "hidden": false
                },
                {
                    "type": "perf",
                    "section": "Performance",
                    "hidden": false
                },
                {
                    "type": "test",
                    "section": "Testing",
                    "hidden": false
                },
                {
                    "type": "ci",
                    "section": "CI",
                    "hidden": false
                },
                {
                    "type": "build",
                    "section": "Build",
                    "hidden": false
                },
                {
                    "type": "revert",
                    "section": "Reverts",
                    "hidden": false
                }
            ],
            "extra-files": [],
            "exclude-changes-in-changelog-regexp": "--no-changelog$"
        }
    }
}